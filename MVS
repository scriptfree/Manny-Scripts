--Murderers VS Sheriffs

local Library = loadstring(game:HttpGet("https://pastebin.com/raw/GdN5iRbt"))() 
local Window = Library.CreateLib("Manny Scripts", "DarkTheme") 
local Tab1 = Window:NewTab("Autofarms")
local Tab1Section = Tab1:NewSection("Main")


local function teleportToPlayer(player)
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = player.Character
    if character and targetCharacter then
        local rootPart = character:FindFirstChild("HumanoidRootPart")
        local targetRootPart = targetCharacter:FindFirstChild("HumanoidRootPart")
        if rootPart and targetRootPart then
            rootPart.CFrame = targetRootPart.CFrame
        end
    end
end

local function moveToPlayer(targetPlayer)
    local localCharacter = game.Players.LocalPlayer.Character
    local targetCharacter = targetPlayer.Character
    if localCharacter and targetCharacter then
        local localRootPart = localCharacter:FindFirstChild("HumanoidRootPart")
        local targetRootPart = targetCharacter:FindFirstChild("HumanoidRootPart")
        if localRootPart and targetRootPart then
            localRootPart.CFrame = targetRootPart.CFrame
        end
    end
end

local players = game:GetService("Players")

local playerService = game:GetService("Players")


Tab1Section:NewToggle("Teleport to all sheriffs", "teleports you to every sheriff", function(t)
getgenv().FLO = t
while getgenv().FLO and wait(1) do


for _, player in ipairs(players:GetPlayers()) do
    if player.Team and player.Team.Name == "Sheriffs" then
        teleportToPlayer(player)
        break
    end
end


end
end)

Tab1Section:NewToggle("Teleport to all murderers", "teleports you to every murderer", function(t)
getgenv().FLO = t
while getgenv().FLO and wait(1) do


for _, player in ipairs(playerService:GetPlayers()) do
    if player.Team and player.Team.Name == "Murderers" then
        moveToPlayer(player)
        break
    end
end

end
end)
